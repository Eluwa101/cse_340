<!-- It should have the normal components as all the other views and also be able to display a flash message and errors. -->
<title><%= title %></title>
<h1><%= title %></h1>

<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<h2>Welcome <%= account_firstname %>!</h2>
